{
  "extends": [
    "airbnb-base",
    "plugin:node/recommended"
  ],
  "rules": {
    "linebreak-style": 0,
    "comma-dangle": "off",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off",
    "no-tabs": "off",
    "indent": [
      "error",
      "tab",
      {
        "SwitchCase": 1
      }
    ]
  }
}